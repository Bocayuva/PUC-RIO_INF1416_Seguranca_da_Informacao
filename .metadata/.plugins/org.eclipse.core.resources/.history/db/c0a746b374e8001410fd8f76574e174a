public class QuartaEtapa extends Home {

	private JFrame frame;
	private Usuario usuario;	
	
	public QuartaEtapa(JFrame jframe, Usuario jusuario){		
		frame = jframe;
		usuario = jusuario;
	}
	
	public void begin(){
		
		frame.getContentPane().removeAll();		
		
		JPanel panel = new JPanel();
		frame.getContentPane().add(panel, BorderLayout.CENTER);
		
		JLabel lbBoasVindas = new JLabel("Ol√°, " + usuario.getUser_name());
		panel.add(lbBoasVindas);
		
		final JLabel lbMsgErro = new JLabel("-");
		panel.add(lbMsgErro);
				
		Home.rebuildFrame();
		
	}
	
}
