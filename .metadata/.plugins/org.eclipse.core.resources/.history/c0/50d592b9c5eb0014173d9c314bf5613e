package main.helper;

import java.io.FileWriter;

public class File {

	public static void adicionaFinalArquivo(String filename, String new_line){
      try{
        FileWriter fw = new FileWriter(filename,true);
        fw.write(new_line);
        fw.close();
      }catch(Exception e){
        System.out.println("=> Erro ao adicionar digest com status NOT FOUND! ");
      }      

    }
	
}
