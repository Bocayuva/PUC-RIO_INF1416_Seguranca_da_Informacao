package main.java;

import javax.swing.JButton;
import javax.swing.JFrame;

import main.business.Usuario;

public class CadastrarUsuario extends Menu {

	public CadastrarUsuario(Usuario fusuario, JFrame fframe) {
		super(fusuario, fframe);		
	}

	public void montaFormCadastro() {
		super.allPane.remove(super.corpo2);
		
		JButton btnTeste = new JButton("teste");
		super.corpo2.add(btnTeste);
	}

}
